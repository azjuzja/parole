{
	vector <int> marks{ 3,8,7,4 };
	vector <string> names{ "Anna","Antons","Vlads","Artemij" };
	int mark;
	
	cin >> mark ;
	if (marks[0] == mark)cout << names[0];
	if (marks[1] == mark)cout << names[1];
